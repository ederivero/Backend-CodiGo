{
  "openapi": "3.0.3",
  "info": {
    "title": "Nuestra primera API con Flask",
    "description": "Api para el consumo de base de datos con Flask y PostgreSQL",
    "contact": {
      "email": "correo@swagger.io"
    },
    "license": {
