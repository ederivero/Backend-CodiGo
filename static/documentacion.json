    },
    "version": "1.0.11"
  },
  "externalDocs": {
    "description": "Busca mas informacion",
    "url": "http://swagger.io"
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    }
  ],
  "tags": [
    {
      "name": "trabajador",
      "description": "Todo lo relacionado con los trabajadores"
    }
  ],
  "paths": {
    "/trabajadores": {
      "get": {
        "tags": [
          "trabajador"
        ],
        "summary": "Listar todos los trabajadores",
        "description": "Devolver todos los trabajadores",
        "operationId": "listarTrabajadores",
        "responses": {
          "200": {
            "description": "Operacion exitosa",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Trabajadores"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Trabajadores": {
        "type": "object",
        "properties": {
          "message": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Trabajador"
            }
          }
        }
      },
      "Trabajador": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int",
            "example": 1
          },
          "nombre": {
            "type": "string",
            "example": "Juanito"
          },
          "habilitado": {
            "type": "boolean",
            "example": true
          }
        }
      }
    }
  }
}